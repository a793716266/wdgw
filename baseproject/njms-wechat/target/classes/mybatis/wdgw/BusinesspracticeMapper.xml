<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flc.dao.BusinesspracticeDao">
	<select id="findAll" resultType="com.flc.entity.Businesspractice">
		SELECT
		  `BUSINESSPRACTICE_ID`,
		  `TITLENAME`,
		  `PROJECTCOUNT`,
		  `MAXSALARY`,
		  `AVGSALARY`,
		  `JOBPERCENT`,
		  `STATE`
		FROM `wdgw`.`wdgw_businesspractice`
	</select>
</mapper>