<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flc.dao.HrinfoDao">
	<!-- 查询HR对学生的评价（3个）-->
	<select id="selectAllHRInfo" resultType="com.flc.entity.Hrinfo">
		SELECT   `HEADPORTRAIT` as HEADPORTRAIT,
		`HR_NAME`,`EVALUATION`,`HR_POSITION`,`LOGO` AS LOGO
		FROM 
		`wdgw_hrinfo`                                    
		LIMIT 0,3
	</select>
	
	
	
</mapper>