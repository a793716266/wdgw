<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flc.dao.CompanyDao">
	<select id="getAllCompany" resultType="com.flc.entity.Company">
		SELECT
 		 `COMPANY_ID`,`NAME`,`PICTURE` as picture
		FROM `wdgw`.`wdgw_company`
		order by CREATETIME desc
	</select>
</mapper>